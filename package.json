{
    "name": "express-ts-forum-api",
    "version": "0.1.0",
    "description": "API REST Forum",
    "main": "src/server.ts",
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon",
        "test": "dotenv -e .env.test -- prisma migrate reset && dotenv -e .env.test jest -i --detectOpenHandles --maxWorkers=1",
        "format": "prettier  --write \"src/**/*.ts\"",
        "rollback": "prisma migrate reset",
        "prepare": "husky install"
    },
    "prisma": {
        "seed": "ts-node prisma/seeders"
    },
    "keywords": [
        "Express",
        "Typescript",
        "API",
        "REST",
        "Forum"
    ],
    "author": "Santos Pierre",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^3.11.1",
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.2.163",
        "bcrypt": "^5.0.1",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv-cli": "^5.1.0",
        "dotenv-flow": "^3.2.0",
        "express": "^4.17.3",
        "express-async-errors": "^3.1.1",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@faker-js/faker": "^6.1.1",
        "@swc/jest": "^0.2.20",
        "@trivago/prettier-plugin-sort-imports": "^3.2.0",
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/dotenv-flow": "^3.2.0",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.1",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/morgan": "^1.9.3",
        "@types/node": "^17.0.23",
        "@types/supertest": "^2.0.12",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "nodemon": "^2.0.15",
        "prettier": "^2.6.1",
        "prisma": "^3.11.1",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.4",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.14.1",
        "typescript": "^4.6.3"
    }
